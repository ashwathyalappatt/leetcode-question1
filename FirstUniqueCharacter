class Solution {
public:
    int firstUniqChar(string s) {
        unordered_map<char,int>nmap;
        if(s.empty())
            return -1;
        for(int i =0;i<s.size();i++)
        {
            nmap[s[i]]++;
        }
        for(int j=0;j<s.size();j++)
        {
            if(nmap[s[j]] == 1)
                return j;
        }
        return -1;
        
    }
};
